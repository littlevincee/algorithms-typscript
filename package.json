  "author": {
    "name": "vinson wong",
    "email": "vinsonwong15@gmail.com"
  },
  "main": "dist/bundle.js",
  "types": "dist/bundle.d.ts",
  "files": [
    "dist/bundle.d.ts",
    "dist/bundle.js",
    "dist/bundle.js.map"
  ],
  "scripts": {
    "build:tsc": "tsc",
    "build": "npm run clear && npm run build:tsc && npm run bundle",
    "bundle": "rollup --config",
    "clear": "rm -rf dist",
    "dev:debug": "ts-node-dev --transpile-only --respawn --inspect=4321 --project tsconfig.json src/app.ts",
    "dev": "ts-node-dev --transpile-only --respawn --project tsconfig.json src/app.ts",
    "start": "node dist/app.js",
    "prestart": "npm run clear && npm run build",
    "test": "jest"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.31",
    "jest": "^28.0.3",
    "nodemon": "^2.0.16",
    "rollup": "^2.72.0",
    "rollup-plugin-dts": "^4.2.1",
    "ts-jest": "^28.0.1",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.4"
  }
}
